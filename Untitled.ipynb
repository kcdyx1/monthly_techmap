    "from googletrans import Translator\n",
    "translator = Translator()\n",
    "\n",
    "with open(\"/Users/kangchen/Downloads/xu_indictment.txt\", 'r') as f:\n",
    "    lines = f.readlines()\n",
    "\n",
    "for line in lines[0:2]:\n",
    "    l = line.strip().replace('\\n','')\n",
    "    i = translator.translate(l, dest='zh-cn').text\n",
    "    print(i)"
